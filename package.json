{
  "name": "boilerplate_react_native_expo",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Ã‰verton Toffanetto",
  "description": "This application is a boilerplate for complex and large systems, especially those that need to scale with security and code quality.",
  "repository": "https://github.com/everton-dgn/boilerplate_react_native_expo",
  "main": "src/App.tsx",
  "scripts": {
    "dev": "expo start -c --tunnel --go",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build": "eas build",
    "build:local": "eas build --local",
    "lint": "eslint --cache src --ext ts,tsx --max-warnings=0",
    "lint:fix": "eslint --cache --fix src --ext ts,tsx",
    "format": "prettier --cache --write src",
    "check:format": "prettier --cache --check src",
    "typecheck": "tsc --noEmit",
    "build:check": "cd android && ./gradlew build --warning-mode all && cd ..",
    "expo:doctor": "npx expo-doctor",
    "check:update": "npx upgrade-interactive --latest",
    "update:expo": "npx add expo@latest",
    "update:fix": "npx expo install --fix"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo-google-fonts/nunito": "^0.2.3",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "axios": "^1.6.7",
    "expo": "^49.0.21",
    "expo-application": "^5.8.3",
    "expo-dev-client": "~2.4.12",
    "expo-font": "~11.4.0",
    "expo-secure-store": "^12.8.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "i18next": "^23.7.16",
    "react": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-i18next": "^14.0.0",
    "react-native": "0.72.6",
    "react-native-paper": "^5.12.0",
    "react-native-responsive-screen": "^1.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "babel-plugin-module-resolver": "^5.0.0",
    "react-native-svg-transformer": "^1.2.0",
    "typescript": "^5.3.3"
  }
}
